{
  "debug": true,
  "override": true,
  "data-source": "commits|prs|prs-with-issues",
  "milestone-match": "{{tag_name}}",
  "include-messages" : "all",
  "changelogFilename": "CHANGELOG.md",
  "generate": true,
  "groupBy": {
    "Enhancements:": ["enhancement", "documentation", "dependencies"],
    "Bug Fixes:": ["bug"]
  },
  "template": {
    "issue": "- {{labels}} {{name}} [{{text}}]({{url}}) \n\t{{body}}",
    "label": "[**{{label}}**]",
    "noLabel": "closed",
    "group": "\n#### {{heading}}\n",
    "changelogTitle": "# Changelog\n\n",
    "release": "## {{release}} ({{date}})\n{{body}}",
    "releaseSeparator": "\n---\n\n"
  }
}
